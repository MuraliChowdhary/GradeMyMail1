  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"vite\"",
    "dev:frontend": "vite",
    "dev:server": "tsx watch server/index.ts",
    "dev:server:mock": "node server/quick-server.cjs",
    "dev:full": "concurrently \"npm run dev:server\" \"npm run dev\"",
    "start": "node scripts/start-with-models.js",
    "setup:ollama": "bash scripts/setup-ollama.sh",
    "test:hybrid-ai": "node scripts/test-hybrid-ai.js",
    "test:professional-ai": "node scripts/test-professional-ai.js",
    "server": "tsx server/index.ts",
    "build": "vite build",
    "build:check": "tsc -b && vite build",
    "build:server": "tsc -p server/tsconfig.production.json --skipLibCheck",
    "build:server:production": "tsc -p server/tsconfig.production.json --skipLibCheck",
    "build:analyze": "npm run build && open dist/stats.html",
    "build:production": "vite build --config vite.config.simple.ts && node scripts/compress-assets.js",
    "build:production:analyze": "npm run build:production && open dist/stats.html",
    "build:production:script": "node scripts/build-production.js",
    "compress": "node scripts/compress-assets.js",
    "verify:build": "node scripts/verify-production-build.js",
    "deploy": "bash scripts/deploy.sh",
    "start:server": "node dist/server/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "vite preview",
    "prepare": "husky",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:component": "cypress run --component",
    "cypress:run:e2e": "cypress run --e2e",
    "test:e2e": "start-server-and-test dev http://localhost:5173 cypress:run:e2e",
    "test:component": "cypress run --component",
    "test:all": "npm run test:run && npm run test:component && npm run test:e2e",
    "chromatic": "npx chromatic --project-token=$CHROMATIC_PROJECT_TOKEN",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
